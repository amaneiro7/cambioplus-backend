{
  "name": "cambioplus-backend-api",
  "version": "1.0.0",
  "description": "backend para controlar el llamado de la api de binance",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/indx.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "api",
    "rest",
    "nodejs",
    "sql",
    "backend"
  ],
  "author": "Andres Maneiro <amaneiro7@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "mysql2": "^3.3.1",
    "sequelize": "^6.31.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "standard": "^17.0.0"
  },
  "eslintConfig": {
    "extends": "node_modules/standard/eslintrc.json"
  }
}
